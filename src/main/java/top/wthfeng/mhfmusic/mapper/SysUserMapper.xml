<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.wthfeng.mhfmusic.dao.SysUserDAO" >

    <!--系统用户登录-->
    <select id="login" parameterType="java.util.Map" resultType="java.lang.String">
        SELECT a.realname from admin a WHERE a.username=#{username} and a.password=#{password}
    </select>
</mapper>